{
  "name": "ahafh",
  "version": "1.0.0",
  "description": "An AirBNB clone",
  "engines": {
    "node": "10.3.0",
    "npm": "6.8.0"
  },
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "server": "nodemon app.js",
    "debug": "node --inspect app.js",
    "frontend-install": "npm install --prefix frontend/src/components",
    "frontend": "npm start --prefix frontend/src/components",
    "dev": "concurrently \"npm run server\" \"npm run frontend\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix frontend && npm run build --prefix frontend"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "aws-s3": "^2.0.5",
    "axios": "^0.18.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "concurrently": "^4.1.0",
    "crypto-js": "^3.1.9-1",
    "eslint": "5.12.0",
    "express": "^4.16.4",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^2.2.0",
    "mongodb": "^3.2.3",
    "mongoose": "^5.4.19",
    "node-sass": "^4.11.0",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "react": "^16.8.5",
    "react-dom": "^16.8.5",
    "react-geosuggest": "^2.10.0",
    "react-google-maps": "^9.4.5",
    "react-redux": "^6.0.1",
    "react-router-dom": "^5.0.0",
    "react-s3": "^1.3.1",
    "react-scripts": "^2.1.3",
    "react-star-rating-component": "^1.4.1",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "validator": "^13.7.0"
  },
  "devDependencies": {
    "nodemon": "^1.18.10"
  }
}
